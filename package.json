{
  "name": "minimal-reactjs-boilerplate",
  "version": "1.0.0",
  "description": "Create React app without CLI",
  "main": "index.js",
  "author": "Raman Zhminko",
  "license": "ISC",
  "scripts": {
    "clear": "rimraf build",
    "build:server": "webpack --config webpack.server.config.js",
    "start:server": "node build/server/bundle.js",
    "dev:server": "nodemon --ignore build --exec \"run-s clear build:server start:server\"",
    "dev:client": "webpack serve --config webpack.client.config.js",
    "dev": "run-p dev:client dev:server"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "mini-css-extract-plugin": "^2.4.5",
    "node-sass": "^7.0.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.44.0",
    "sass-loader": "^12.4.0",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0",
    "yarn-run-all": "^3.1.1"
  },
  "dependencies": {
    "style-loader": "^3.3.1"
  }
}
