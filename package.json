    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/runtime": "^7.16.7",
    "babel-loader": "^8.2.3",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^6.5.1",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "mini-css-extract-plugin": "^2.4.5",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.44.0",
    "sass-loader": "^12.4.0",
    "source-map-loader": "^3.0.1",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
