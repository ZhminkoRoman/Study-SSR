    "@reduxjs/toolkit": "^1.7.2",
    "cors": "^2.8.5",
    "jest": "^27.5.0",
    "nodemon-webpack-plugin": "^4.7.0",
    "query-string": "^7.1.1",
    "react-bootstrap": "^2.1.1",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "react-scroll-to-bottom": "^4.2.0",
